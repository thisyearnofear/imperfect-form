{
    "version": 2,
    "builds": [
        {
            "src": "api/websocket.js",
            "use": "@vercel/edge"
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/websocket",
            "dest": "/api/websocket.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/[^.]+",
            "dest": "/index.html"
        }
    ]
}